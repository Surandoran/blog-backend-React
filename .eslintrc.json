{
    "env": {
        "commonjs" : true,
        "es6" : true,
        "node" : true
    },
    "extends" : ["eslint:recommended", "prettier"],
    "globals" : {
        "Atomics" : "readonly",
        "SharedArrayBuffer" : "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars" : "warn",
        "no-console" : "off"
    }
}
